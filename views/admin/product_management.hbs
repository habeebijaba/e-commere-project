<div id="layoutSidenav_content">
                <main>
                    <div class="container-fluid px-4">

<div class="container">
    <div class="row">
     <a class="btn mt-5 btn-success ml-auto" style="width:10rem ;" href="/admin/add-product">Add Products</a>
    
        </div>
    <div class="row d-block ">
       
        
    
        <table class="table mt-5 " id="viewusers">
            <thead>
                <tr>
                    
                    <th scope="col">Customer Id</th>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                    <th scope="col">Edit-User</th>
                    <th scope="col">Delete-User</th>
                    <th scope="col">Delete-User</th>

                </tr>
            </thead>
            <tbody>
                {{#each allUsers}}
                <tr>
                    
                    <th scope="row">{{_id}}</th>
                    <td>{{this.name}}</td>
                    <td>{{this.email}}</td>
                    <td><a href="/admin/edit-user/{{this.email}}" onclick="" class="btn btn-primary">Edit User</a></td>
                    <td><a href="/admin/delete-user/{{this.email}}" onclick="return confirm('delete user {{this.name}}')" class="btn btn-danger">Delete User</a></td>
                    <td><img style=" height:50px"  id="z" src="/product-images/{{this._id}}.jpg" alt="" ></td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>
